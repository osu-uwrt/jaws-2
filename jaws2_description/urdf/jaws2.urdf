<robot name="jaws2">

  <material name="delrin">
    <color rgba=".8 .8 .75 1" />
  </material>

  <material name="aluminum">
    <color rgba=".68 .7 .74 1" />
  </material>

  <material name="light">
     <color rgba=".5 .5 .5 1" />
  </material>

  <material name="dark">
     <color rgba=".4 .4 .4 1" />
  </material>

 <gazebo reference="base_link">
    <material>Gazebo/Grey</material>
 </gazebo>

<!--BASE-->

  <link name="base_link">
    
    <inertial>
      <origin xyz="0.0077 0 0" rpy="0 1.57 0" />

      <mass value="4.31" />

      <inertia ixx="0.063" ixy="0.0" ixz="0.0" iyy="0.014" iyz="0.0" izz="0.063"/>
    </inertial>

    <visual>
      <origin xyz="0 0 0" rpy="0 1.57 0" />

      <geometry>
        <cylinder radius="0.076" length="0.375" />
      </geometry>

      <material name="aluminum" />
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 1.57 0" />

      <geometry>
        <cylinder radius="0.076" length="0.375" />
      </geometry>
    </collision>
  </link>

  <gazebo reference="base_link">
    <material>Gazebo/Grey</material>
  </gazebo>
  
  
<!--BASE DOME-->

  <link name="base_dome">
    <visual>
      <geometry>
        <sphere radius="0.076" />
      </geometry>

      <material name="delrin" />
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />

      <mass value="0.77" />

      <inertia ixx="0.003" ixy="0.0" ixz="0.0" iyy="0.003" iyz="0.0" izz="0.003"/>
    </inertial>
  </link>

<!--THRUSTERS-->

<!--AFT-->
  <link name="aft_base">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />

      <geometry>
        <box size="0.04 0.02 0.03" />
      </geometry>

      <material name="dark" />
    </visual>
  </link>

  <link name="aft_body">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />

      <geometry>
        <cylinder radius="0.022" length=".106" />
      </geometry>

      <material name="light" />
    </visual>

       <inertial>
       <origin xyz="0 0 0" rpy="0 0 0" />

      <mass value="0.549" />

      <inertia ixx="0" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>

  <link name="aft_dome">
    <visual>
      <geometry>
        <sphere radius="0.022" />
      </geometry>

      <material name="dark" />
    </visual>
  </link>

  <link name="aft_duct">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.04" />
      </geometry>

      <material name="dark" />
    </visual>
  </link>




<!--PORT-->
  <link name="port_base">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0" />

      <geometry>
        <cylinder radius="0.01" length="0.06" />
      </geometry>

      <material name="dark" />
    </visual>
  </link>

  <link name="port_body">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />

      <geometry>
        <cylinder radius="0.022" length=".106" />
      </geometry>

      <material name="light" />
    </visual>

       <inertial>
       <origin xyz="0 0 0" rpy="0 0 0" />

      <mass value="0.549" />

      <inertia ixx="0" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>

  <link name="port_dome">
    <visual>
      <geometry>
        <sphere radius="0.022" />
      </geometry>

      <material name="dark" />
    </visual>
  </link>

  <link name="port_duct">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.04" />
      </geometry>

      <material name="dark" />
    </visual>
  </link>



<!--STARBOARD-->

  <link name="stbd_base">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0" />

      <geometry>
        <cylinder radius="0.01" length="0.06" />
      </geometry>

      <material name="dark" />
    </visual>
  </link>

  <link name="stbd_body">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />

      <geometry>
        <cylinder radius="0.022" length=".106" />
      </geometry>

      <material name="light" />
    </visual>

       <inertial>
       <origin xyz="0 0 0" rpy="0 0 0" />

      <mass value="0.549" />

      <inertia ixx="0" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>

  <link name="stbd_dome">
    <visual>
      <geometry>
        <sphere radius="0.022" />
      </geometry>

      <material name="dark" />
    </visual>
  </link>

  <link name="stbd_duct">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.04" />
      </geometry>

      <material name="dark" />
    </visual>
  </link>


  <joint name="base-dome" type="fixed">
    <origin xyz="0.1875 0 0" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="base_dome" />
  </joint>

  <joint name="base-aft" type="fixed">
    <origin xyz="-0.11 0 0.076" rpy="3.14 1.57 0" />
    <parent link="base_link" />
    <child link="aft_base" />
  </joint>

  <joint name="aft-body" type="fixed">
    <origin xyz="-0.05 0 0" rpy="0 0 0"/>
    <parent link="aft_base" />
    <child link="aft_body" />
  </joint>

  <joint name="aft-dome" type="fixed">
    <origin xyz="0 0 -0.053" rpy="0 0 0"/>
    <parent link="aft_body" />
    <child link="aft_dome" />
  </joint>

  <joint name="aft-duct" type="fixed">
    <origin xyz="0 0 0.04" rpy="0 0 0"/>
    <parent link="aft_body" />
    <child link="aft_duct" />
  </joint>

  <joint name="port-base" type="fixed">
    <origin xyz="0.06 0.06 0" rpy="0 0 0"/>
    <parent link="base_link" />
    <child link="port_base" />
  </joint>

  <joint name="port-body" type="fixed">
    <origin xyz="0 0.05 0" rpy="0 0 0"/>
    <parent link="port_base" />
    <child link="port_body" />
  </joint>

  <joint name="port-dome" type="fixed">
    <origin xyz="0 0 -0.053" rpy="0 0 0"/>
    <parent link="port_body" />
    <child link="port_dome" />
  </joint>

  <joint name="port-duct" type="fixed">
    <origin xyz="0 0 0.04" rpy="0 0 0"/>
    <parent link="port_body" />
    <child link="port_duct" />
  </joint>

  <joint name="stbd-base" type="fixed">
    <origin xyz="0.06 -0.06 0" rpy="0 0 3.14159"/>
    <parent link="base_link" />
    <child link="stbd_base" />
  </joint>

  <joint name="stbd-body" type="fixed">
    <origin xyz="0 0.05 0" rpy="0 0 0"/>
    <parent link="stbd_base" />
    <child link="stbd_body" />
  </joint>

  <joint name="stbd-dome" type="fixed">
    <origin xyz="0 0 -0.053" rpy="0 0 0"/>
    <parent link="stbd_body" />
    <child link="stbd_dome" />
  </joint>

  <joint name="stbd-duct" type="fixed">
    <origin xyz="0 0 0.04" rpy="0 0 0"/>
    <parent link="stbd_body" />
    <child link="stbd_duct" />
  </joint>

  <gazebo reference="base_link">
  <plugin name="BuoyancyPlugin" filename="libBuoyancyPlugin.so">
  </plugin>
  </gazebo>

</robot>
